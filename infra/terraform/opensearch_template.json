{
  "index_patterns": [
    "cali-logs-*"
  ],
  "template": {
    "mappings": {
      "properties": {
        "timestamp": {
          "type": "date"
        },
        "level": {
          "type": "keyword"
        },
        "service": {
          "type": "keyword"
        },
        "message": {
          "type": "text"
        }
      }
    }
  }
}